@model IEnumerable<SanAgustin.Models.Bautizos>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-bordered table-hover table-condensed" id="">
    <tr>
        <th>
           <h3>Registro de Bautizos</h3>
        </th>
        
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Bautizado)
            </td>
            
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.BautizoId }) |
                @Html.ActionLink("Details", "Details", new { id = item.BautizoId }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.BautizoId })
            </td>
        </tr>
    }
</table>
